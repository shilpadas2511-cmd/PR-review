<process xmlns="http://www.hybris.de/xsd/processdefinition" start="sendQuoteToCpq"
  name="sap-cpq-quote-common-validate-submit-process"
  processClass="de.hybris.platform.commerceservices.model.process.QuoteProcessModel">
    <action id="sendQuoteToCpq" bean="partnerQuoteValidationAction">
        <transition name="OK" to="partnerQuoteSubmitAction"/>
        <transition name="NOK" to="failed"/>
    </action>

    <action id="partnerQuoteSubmitAction" bean="partnerQuoteSubmitAction">
        <transition name="OK" to="success"/>
        <transition name="NOK" to="failed"/>
    </action>

    <end id="failed" state="FAILED"/>
    <end id="success" state="SUCCEEDED"/>


</process>
