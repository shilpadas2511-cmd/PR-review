<config xmlns="http://www.hybris.com/cockpit/config"
  xmlns:n="http://www.hybris.com/cockpitng/config/explorertree"
  xmlns:na="http://www.hybris.com/cockpitng/config/notifications"
  xmlns:wz="http://www.hybris.com/cockpitng/config/wizard-config"
  xmlns:as="http://www.hybris.com/cockpitng/config/advancedsearch"
  xmlns:advanced-search="http://www.hybris.com/cockpitng/config/advancedsearch"
  xmlns:ss="http://www.hybris.com/cockpitng/config/simplesearch"
  xmlns:y="http://www.hybris.com/cockpit/config/hybris"
  xmlns:lv="http://www.hybris.com/cockpitng/component/listView"
  xmlns:list-view="http://www.hybris.com/cockpitng/component/listView"
  xmlns:ea="http://www.hybris.com/cockpitng/component/editorArea"
  xmlns:ln="http://www.hybris.com/cockpitng/config/links"
  xmlns:df="http://www.hybris.com/cockpitng/component/dynamicForms">
    <context component="global-links">
        <ln:links>
            <ln:link label="Privacy Policy" target="_blank"
              url="https://w3.ibm.com/w3publisher/w3-privacy-notice"/>
        </ln:links>
    </context>

    <context parent="GenericItem" type="IbmDeploymentType" component="listview">
        <list:list-view xmlns:list="http://www.hybris.com/cockpitng/component/listView">
            <list:column qualifier="code"/>
            <list:column qualifier="sapCode"/>
        </list:list-view>
    </context>

    <context type="IbmDeploymentType" component="base">
        <y:base xmlns:y="http://www.hybris.com/cockpit/config/hybris">
            <y:labels>
                <y:label>code + ' - ' + sapCode</y:label>
            </y:labels>
        </y:base>
    </context>

    <context type="IbmDeploymentType" component="simple-search">
        <ss:simple-search>
            <ss:field name="code"/>
            <ss:field name="sapCode"/>
        </ss:simple-search>
    </context>

    <context parent="GenericItem" type="SellerAudienceMask" component="listview">
        <list:list-view xmlns:list="http://www.hybris.com/cockpitng/component/listView">
            <list:column qualifier="code"/>
        </list:list-view>
    </context>

    <context type="SellerAudienceMask" component="base">
        <y:base xmlns:y="http://www.hybris.com/cockpit/config/hybris">
            <y:labels>
                <y:label>code</y:label>
            </y:labels>
        </y:base>
    </context>

    <context type="SellerAudienceMask" component="simple-search">
        <ss:simple-search>
            <ss:field name="code"/>
        </ss:simple-search>
    </context>


    <context parent="GenericItem" type="IbmRevenueStream" component="listview">
        <list:list-view xmlns:list="http://www.hybris.com/cockpitng/component/listView">
            <list:column qualifier="code"/>
        </list:list-view>
    </context>

    <context type="IbmRevenueStream" component="base">
        <y:base xmlns:y="http://www.hybris.com/cockpit/config/hybris">
            <y:labels>
                <y:label>code</y:label>
            </y:labels>
        </y:base>
    </context>

    <context type="IbmRevenueStream" component="simple-search">
        <ss:simple-search>
            <ss:field name="code"/>
            <ss:field name="name"/>
        </ss:simple-search>
    </context>

    <context type="QuoteState" component="base">
        <y:base xmlns:y="http://www.hybris.com/cockpit/config/hybris">
            <y:labels>
                <y:label>code+ ' - ' +name</y:label>
            </y:labels>
        </y:base>
    </context>
	
	
	<context parent="GenericItem" type="PartnerSpecialBidReason" component="listview">
        <list:list-view xmlns:list="http://www.hybris.com/cockpitng/component/listView">
            <list:column qualifier="code"/>
			<list:column qualifier="name"/>
        </list:list-view>
    </context>

    <context type="PartnerSpecialBidReason" component="base">
        <y:base xmlns:y="http://www.hybris.com/cockpit/config/hybris">
            <y:labels>
                <y:label>code</y:label>
            </y:labels>
        </y:base>
    </context>

    <context component="explorer-tree" merge-by="module">
        <n:explorer-tree>
            <n:navigation-node id="SpecialBid">
                <n:type-node code="PartnerSpecialBidReason" id="SpecialBidReason"/>
                <n:type-node code="PartnerQuestions" id="SpecialBidQuestions"/>
                <n:type-node code="PartnerSpecialBidReasonToQuestionMapping"
                  id="ReasonQuestionMapping"/>
            </n:navigation-node>
        </n:explorer-tree>
    </context>
    <context type="PartnerSpecialBidReason" component="simple-search" merge-by="type" parent="GenericItem">
        <ss:simple-search>
            <ss:field name="code" />
            <ss:field name="name"/>
        </ss:simple-search>
    </context>

    <context type="PartnerSpecialBidReason" component="advanced-search" merge-by="type" parent="GenericItem">
        <advanced-search:advanced-search>
            <advanced-search:field-list>
                <advanced-search:field name="code" operator="contains" selected="true" />
                <advanced-search:field name="name" operator="contains" selected="true" />
                <advanced-search:field name="active" operator="contains" selected="true" />
            </advanced-search:field-list>
        </advanced-search:advanced-search>
    </context>

    <context type="PartnerQuestions" component="simple-search" merge-by="type" parent="GenericItem">
        <ss:simple-search>
            <ss:field name="code"/>
        </ss:simple-search>
    </context>

    <context type="PartnerQuestions" component="advanced-search" merge-by="type"
      parent="GenericItem">
        <advanced-search:advanced-search>
            <advanced-search:field-list>
                <advanced-search:field name="code" operator="contains" selected="true"/>
                <advanced-search:field name="name" operator="contains" selected="true"/>
            </advanced-search:field-list>
        </advanced-search:advanced-search>
    </context>

    <context parent="GenericItem" type="PartnerQuestions" component="listview">
        <list:list-view xmlns:list="http://www.hybris.com/cockpitng/component/listView">
            <list:column qualifier="code"/>
            <list:column qualifier="name"/>
        </list:list-view>
    </context>

    <context type="PartnerQuestions" component="base">
        <y:base xmlns:y="http://www.hybris.com/cockpit/config/hybris">
            <y:labels>
                <y:label>code</y:label>
            </y:labels>
        </y:base>
    </context>

    <context type="PartnerSpecialBidReasonToQuestionMapping" component="advanced-search"
      merge-by="type"
      parent="GenericItem">
        <advanced-search:advanced-search>
            <advanced-search:field-list>
                <advanced-search:field name="reason" operator="equals" selected="true"/>
                <advanced-search:field name="question" operator="equals" selected="true"/>
            </advanced-search:field-list>
        </advanced-search:advanced-search>
    </context>

    <context parent="GenericItem" type="PartnerSpecialBidReasonToQuestionMapping"
      component="listview">
        <list:list-view xmlns:list="http://www.hybris.com/cockpitng/component/listView">
            <list:column qualifier="reason"/>
            <list:column qualifier="question"/>
        </list:list-view>
    </context>

    <context type="PartnerSpecialBidReasonToQuestionMapping" component="base">
        <y:base xmlns:y="http://www.hybris.com/cockpit/config/hybris">
            <y:labels>
                <y:label>reason</y:label>
            </y:labels>
        </y:base>
    </context>
    <context type="PartnerQuestionsSelection" component="base">
        <y:base xmlns:y="http://www.hybris.com/cockpit/config/hybris">
            <y:labels>
                <y:label>question!=null ? question.code : pk</y:label>
            </y:labels>
        </y:base>
    </context>

    
    <context type="CPQOrderEntryProductInfo" component="base">
        <y:base xmlns:y="http://www.hybris.com/cockpit/config/hybris">
            <y:labels>
                <y:label>cpqCharacteristicName</y:label>
            </y:labels>
        </y:base>
    </context>
